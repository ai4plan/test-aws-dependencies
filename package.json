{
  "name": "test-aws-dependencies",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@types/node": "24.10.1",
    "typescript": "5.9.3",
    "graphviz-cli": "2.0.0"
  },
  "scripts": {
    "install-ai4plan-client":              "cd ai4plan-client              && npm i",
    "install-test-aws-amplify":            "cd test-aws-amplify            && npm i",
    "install-test-aws-amplify-ui-react":   "cd test-aws-amplify-ui-react   && npm i",
    "install-all": "npm run install-ai4plan-client && npm run install-test-aws-amplify && npm run install-test-aws-amplify-ui-react",

    "clean-install-ai4plan-client":              "cd ai4plan-client              && npm run clean-install",
    "clean-install-test-aws-amplify":            "cd test-aws-amplify            && npm run clean-install",
    "clean-install-test-aws-amplify-ui-react":   "cd test-aws-amplify-ui-react   && npm run clean-install",
    "clean-install-all": "npm run clean-install-ai4plan-client && npm run clean-install-test-aws-amplify && npm run clean-install-test-aws-amplify-ui-react",

    "diff": "code --diff '../ai4plan/client/package.json' 'ai4plan-client/package.json'"
  }
}


